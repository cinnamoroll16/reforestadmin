{
  "version": 2,
  "buildCommand": "cross-env CI=false npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/Sensor",
      "destination": "/sensors",
      "permanent": true
    },
    {
      "source": "/Sensors",
      "destination": "/sensors",
      "permanent": true
    },
    {
      "source": "/Notification",
      "destination": "/notifications",
      "permanent": true
    },
    {
      "source": "/Notifications",
      "destination": "/notifications",
      "permanent": true
    },
    {
      "source": "/Recommendation",
      "destination": "/recommendations",
      "permanent": true
    },
    {
      "source": "/Recommendations",
      "destination": "/recommendations",
      "permanent": true
    },
    {
      "source": "/Dashboard",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/data/Tree_Seedling_Dataset.xlsx",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, must-revalidate"
        },
        {
          "key": "Content-Type",
          "value": "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}